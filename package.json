{
  "dependencies": {
    "liquidjs": "^10.8.4"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch start-app",
    "watch": "chokidar \"views/**/*.liquid\" -c \"npm run render\"",
    "render": "node app.js",
    "start-app": "nodemon app.js",
    "build": "node app.js"
  }
}
